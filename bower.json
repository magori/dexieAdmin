{
  "name": "ng-dexie-admin",
  "description": "A administration interface for dexie, this display the table and the data. You can drop table, load, delte, dump and search in data",
  "license": "MIT",

  "dependencies": {
    "angular": "~1.5.9",
    "angular-route": "~1.5.9",
    "angular-bootstrap": "~2.0.1",
    "bootstrap": "~3.3.6",
    "dexie": "^1.4.1",
    "chance": "^1.0.3",
    "font-awesome": "^4.6.3",
    "font-awesome-animation": "^0.0.9"
  },

  "devDependencies": {
    "angular-mocks": "~1.5.9"
  },

  "main": [
    "dist/dexieAdmin.js"
  ],

  "overrides": {
    "bootstrap": {
      "main": [
        "dist/css/bootstrap.css",
        "dist/fonts/glyphicons-halflings-regular.eot",
        "dist/fonts/glyphicons-halflings-regular.svg",
        "dist/fonts/glyphicons-halflings-regular.ttf",
        "dist/fonts/glyphicons-halflings-regular.woff",
        "dist/fonts/glyphicons-halflings-regular.woff2"
      ]
    },
    "font-awesome": {
      "main": [
        "css/font-awesome.css",
        "font/fontawesome-webfont.eot",
        "font/fontawesome-webfont.svg",
        "font/fontawesome-webfont.ttf",
        "font/fontawesome-webfont.woff",
        "font/fontawesome-webfont.woff2"
      ]
    }
  },
  "resolutions": {
    "angular": "~1.5.3"
  },
  "ignore": [
    "node_modules",
    "bower_components"
  ],
  "authors": [
    { "name": "Dorian Magnin" }
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/magori/ngDexieAdmin"
  },
  "keywords": [
    "dexie",
    "console",
    "admin",
    "interface"
  ]
}
